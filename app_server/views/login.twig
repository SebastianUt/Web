{% extends 'layout.twig' %}
{% block body %}
<div id="fb-root"></div>

<script src="javascripts/loginFB.js"></script>
<button onclick="getInfo()" id = "status" >Get Info</button>
<button onclick="loginFB()" id="login">Login</button>
<div class="fb-login-button" scope="public_profile,email"
  onlogin="checkLoginState();" data-max-rows="1" data-size="large" data-button-type="login_with" data-show-faces="true" data-auto-logout-link="true" data-use-continue-as="true"> </div>

</div>
{% endblock %}
